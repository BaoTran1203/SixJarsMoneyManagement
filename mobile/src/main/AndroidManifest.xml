<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trangiabao.sixjars">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".base.application.MainApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".base.activity.SplashScreenActivity" />
        <activity android:name=".base.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".base.activity.FilterActivity" />
        <activity android:name="com.trangiabao.sixjars.management.add_revenue.AddRevenueActivity" />
        <activity android:name="com.trangiabao.sixjars.management.edit_revenue.EditRevenueActivity"></activity>
    </application>

</manifest>